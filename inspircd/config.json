{
  "name": "InspIRCd Server",
  "version": "latest",
  "slug": "inspircd",
  "description": "InspIRCd IRC server using the official Docker image with default config",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "services",
  "logo": "logo.png",
  "icon": "icon.png",
  "boot": "auto",
  "ports": {
    "6667/tcp": 6667,
    "6697/tcp": 6697
  },
  "map": [
    {
      "type": "data",
      "path": "/inspircd/conf",
      "read_only": false
    }
  ],
  "image": "ghcr.io/d2crashout/hassio-inspircd"
}
